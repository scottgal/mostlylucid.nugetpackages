{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Yarp": "Information"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.AspNetCore": "Warning",
        "Yarp.ReverseProxy": "Information"
      }
    }
  },
  "Gateway": {
    "HttpPort": 8080,
    "AdminBasePath": "/admin",
    "LogLevel": "Information"
  },
  "Database": {
    "Provider": "None"
  }
}
